// Armstrong number
#include<iostream>
using namespace std;
int main(){
    int number,r , sum=0;//r is remainder
    cin>>number;
    int num=number;
    while (num>0){
        r=num%10;
        sum=sum+(r*r*r);
        num=num/10;
    }
    if(sum == number){
        cout<<number<<" is an Armstrong number."<<endl;
    }
    else{
        cout<<number<<" is not an Armstrong number."<<endl;
    }
    return 0;
}
