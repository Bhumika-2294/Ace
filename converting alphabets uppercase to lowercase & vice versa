#include <iostream>
using namespace std;
int main()
{
	char character;
	cout<<"enter character : ";
	cin>>character;
	int n ;
	n = int (character );
    if (n>= 65 && n<=90){
    	n=n+32;
    	
    } 
	else if(n>=97 && n<=122){
		n= n-32 ;	
	}
  else{
	    cout<<"enter a valid character ."<<"\n";
   } 
    cout<<n<<"\n";
	  cout<<"after convertion : "<<char(n);
	
	return 0;
}
